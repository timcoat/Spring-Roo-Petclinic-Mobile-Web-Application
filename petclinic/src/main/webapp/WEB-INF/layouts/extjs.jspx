    	<script type="text/javascript" src="resources/js/PetCompany/listpanel/OwnerList.js"><!-- required for FF3 and Opera --></script>
    	<script type="text/javascript" src="resources/js/PetCompany/listpanel/PetList.js"><!-- required for FF3 and Opera --></script>
    	<script type="text/javascript" src="resources/js/PetCompany/listpanel/VetList.js"><!-- required for FF3 and Opera --></script>
    	<script type="text/javascript" src="resources/js/PetCompany/listpanel/VisitList.js"><!-- required for FF3 and Opera --></script>
		
		<!-- Application JavaScript -->
    	<script type="text/javascript" src="resources/js/PetClinic/workspace.js"><!-- required for FF3 and Opera --></script>
		
		<spring:message code="application_name" var="app_name"/>
		<title id="page-title"><spring:message code="welcome_h3" arguments="${app_name}" /></title>
	</head>
	
  	<body class="tundra spring">
  		<tiles:insertAttribute name="body"/> 
	</body>
</html>
